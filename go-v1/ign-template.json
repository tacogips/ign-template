{
  "name": "go-v1",
  "version": "1.0.0",
  "description": "Go project template with standard layout",
  "author": "tacogips",
  "repository": "https://github.com/tacogips/ign-template",
  "license": "MIT",
  "tags": [
    "go",
    "golang",
    "nix",
    "task"
  ],
  "variables": {
    "DESCRIPTION": {
      "type": "string",
      "description": "Project description",
      "default": "A Golang project"
    },
    "GO_VERSION": {
      "type": "string",
      "description": "Go version",
      "default": "1.25.4"
    },
    "HOMEPAGE": {
      "type": "string",
      "description": "Project homepage URL",
      "default": "https://github.com/user/repo"
    },
    "MODULE_PATH": {
      "type": "string",
      "description": "Go module path (e.g., github.com/user/repo)",
      "required": true
    },
    "PROJECT_NAME": {
      "type": "string",
      "description": "Project name",
      "required": true
    }
  },
  "settings": {
    "ignore_patterns": [
      ".git",
      ".DS_Store"
    ],
    "include_dotfiles": true
  },
  "hash": "ff6f9ad0ad00c4d66d409aede53b900f262f33689256438c804d4aff325b314c"
}