{
  "name": "go-v1",
  "version": "1.0.0",
  "description": "Go project template with standard layout",
  "author": "tacogips",
  "repository": "https://github.com/tacogips/ign-template",
  "variables": {
    "DESCRIPTION": {
      "type": "string",
      "description": "Variable DESCRIPTION",
      "default": "A Go project"
    },
    "GO_VERSION": {
      "type": "int",
      "description": "Variable GO_VERSION",
      "default": 1
    },
    "LICENSE": {
      "type": "string",
      "description": "Variable LICENSE",
      "default": "MIT"
    },
    "MODULE_PATH": {
      "type": "string",
      "description": "Variable MODULE_PATH",
      "required": true
    },
    "PROJECT_NAME": {
      "type": "string",
      "description": "Variable PROJECT_NAME",
      "required": true
    },
    "USE_TASK": {
      "type": "bool",
      "description": "Variable USE_TASK",
      "required": true
    }
  }
}